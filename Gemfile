source 'https://rubygems.org'

gem 'rails', '3.2.19'

gem 'bootstrap-datepicker-rails'
gem 'bootstrap-sass'
gem 'chardinjs-rails'
gem 'haml-rails'
gem 'filterrific'
gem 'jquery-rails'
gem 'pg'
gem 'random_data'
gem 'thin'
gem 'will_paginate'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end

group :development, :test do
  gem 'minitest', '~> 4.3.2'
  gem 'wwtd'

  # supported database adapters
  # gem 'pg' # already specified in default group higher up
  gem 'sqlite3'
  gem 'mysql'
  gem 'mysql2'
end
